PKG_CPPFLAGS = \
        -Iwin64 \
        -I$(LIBRARY_PREFIX_CYG)/include \
	-DR_NO_REMAP -DSTRICT_R_HEADERS

PKG_LIBS = \
        -Iwin64 \
	-L$(LIBRARY_PREFIX_CYG)/lib \
	-lgit2 -lz -liconv \
	-lwinhttp -lws2_32 -lcrypt32 -lole32 -lrpcrt4 -lsecur32

all: clean

clean:
	rm -f $(SHLIB) $(OBJECTS)

.PHONY: all clean
